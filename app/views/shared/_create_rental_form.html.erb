<%= simple_form_for [@bike, @rental] do |f| %>
  <%= f.date_field :start_date, label:"Start date", value: Date.parse(params[:start_date]) %>
  <%= f.date_field :end_date, label:"End date", value: Date.parse(params[:end_date]) %>
  <%= f.button :submit, class:"primary-button mt-4", value: "Confirm rental" %>
<% end %>
